{
  "name": "tuneUp",
  "version": "1.5",
  "manifest_version": 2,
  "description": "BETAv1.5- An obscure song recognition & SoundCloud search automation tool.",
  "homepage_url": "http://kavika.me",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "tuneUp",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [  
    "tabs",
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.tunein.com/*",
        "*://www.soundcloud.com/*"
      ],
      "js": [
        "src/browser_action/whois.js"
      ]
    }
  ]
}